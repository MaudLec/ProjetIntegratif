package princetonPlainsboro;

public abstract class Personne {

    enum Sexe{F,M;}
    
    protected String nom;
    protected String prenom;
    protected Date naissance;
    protected final int numid;
    protected Sexe sexe;
            

    public Personne(String nom, String prenom, Sexe sexe, Date naissance, int numid) {
        this.nom = nom;
        this.prenom = prenom;
        this.naissance = naissance;
        this.numid = numid;
        this.sexe=sexe;
    }

}
